# This is the network config written by 'subiquity'
network:
  renderer: networkd
  ethernets:
    ens160:
      dhcp4: yes
      dhcp6: no
    ens192:
      dhcp4: no
      dhcp6: no
      addresses: 
        - {{ groups['nginx_bridge'][0] }}/24
      routes: 
        - to: {{ groups['apache'][0] }}
          via: {{ groups['nginx_bridge'][0] }}
  version: 2
